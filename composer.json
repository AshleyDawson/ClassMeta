{
    "name": "ashleydawson/class-meta",
    "type": "library",
    "description": "Add arbitrary metadata to classes and their constants by annotation",
    "license": "MIT",
    "keywords": [
        "meta",
        "metadata",
        "class",
        "constant",
        "annotation"
    ],
    "authors": [
        {
            "name": "Ashley Dawson",
            "email": "ashley@ashleydawson.co.uk"
        }
    ],
    "require": {
        "php": ">=5.5",
        "doctrine/annotations": "^1.2",
        "doctrine/cache": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": { "AshleyDawson\\ClassMeta\\": "lib" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
